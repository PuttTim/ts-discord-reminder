		"discord.js": "^12.5.3",
		"firebase-admin": "^9.9.0"
	},
	"engines": {
		"node": ">=0.12"
	},
	"engineStrict": true,
	"devDependencies": {
		"@types/node": "^15.6.1",
		"@types/ws": "^7.4.4",
		"ts-node": "^10.0.0",
		"typescript": "^4.3.2"
	}
}

{
	"name": "ts-assignmentbot",
	"version": "1.0.0",
	"description": "Assignment timer bot",
	"main": "build/app.js",
	"author": "zS1L3NT",
	"scripts": {
		"start": "node build/app.js",
		"dev": "ts-node src/app.ts",
